<div id="<%= dom_id m_variable%>" class="flex flex-row justify-between border-2 border-blue-200 p-4">
  <div id="invar-data" class="flex flex-row justify-normal w-full">
    <div class="flex flex-col w-1/3">
      <strong>Name</strong>
      <p><%= m_variable.name %></p>
    </div>
    <div class="flex flex-col w-1/3">
      <strong>Value</strong>
      <p><%= m_variable.value %></p>
    </div>
    <div class="flex flex-col w-1/3">
      <strong>Inherited from</strong>
      <p><%= m_variable.variable_holder_type.to_s %></p>
    </div>
  </div>
  <div id="invar-actions" class="flex flex-row w-1/4 justify-evenly">
    
    <a href="<%=edit_m_variable_path(m_variable)%>" title="Edit for this <%=controller_name.classify%> only"
        class="fill-blue-600 stroke-blue-600 hover:fill-blue-800 hover:stroke-blue-800 dark:stroke-blue-400 dark:fill-blue-400 dark:hover:stroke-blue-500 dark:hover:fill-blue-500">
      <%= render 'components/icons/up_arrow_icon', size: '2rem'%>
    </a>
    <%
=begin%>
 <%= button_to {title: "Edit for this #{m_variable.variable_holder_type} only",
                                class: "fill-blue-600 stroke-blue-600 hover:fill-blue-800 hover:stroke-blue-800 dark:stroke-blue-400 
                                dark:fill-blue-400 dark:hover:stroke-blue-500 dark:hover:fill-blue-500"} do
        %> 
<%
=end%>
    <a href="<%= self.send("variables_#{m_variable.variable_holder_type.tableize.singularize}_path".to_sym, m_variable.variable_holder_id)%>" data-turbo="false"
        class="stroke-blue-600 hover:stroke-blue-800 dark:stroke-blue-400 dark:hover:stroke-blue-500">
      <%= render 'components/icons/edit_icon',size: '2rem'%>
    </a>
    
    
  </div>
</div>